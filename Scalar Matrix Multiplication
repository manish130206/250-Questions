#include <stdio.h>

int main() {
    
    int m,n,d;
    printf("ENTER ROW & COLLUMN OF ARRAY :");
    scanf("%d %d",&m,&n);
    int arr1[m][n];
    
    printf("ENTER ELEMENT OF 1ST ARRAY :");
    for(int i=0;i<m;i++){
      for(int j=0;j<n;j++){
            scanf("%d",&arr1[i][j]);
      }
    }
    
    printf("ENTER NUMBER FOR MULTIFLICATION :");
    scanf("%d",&d);
    
    
    for(int i=0;i<m;i++){
        for(int j=0;j<n;j++){
            printf("%d ",(arr1[i][j])*d);
            
        }
        printf("\n");
    }
    
    return 0;
}
