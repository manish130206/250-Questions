#include <stdio.h>

int main() {
    
    int m,n;
    printf("ENTER ROW & COLLUMN OF ARRAY :");
    scanf("%d %d",&m,&n);
    int arr1[m][n],arr2[m][n],arr3[m][n];
    
    printf("ENTER ELEMENT OF 1ST ARRAY :");
    for(int i=0;i<m;i++){
      for(int j=0;j<n;j++){
            scanf("%d",&arr1[i][j]);
      }
    }
    
    printf("ENTER ELEMENT OF 2ND ARRAY :");
    for(int i=0;i<m;i++){
       for(int j=0;j<n;j++){
            scanf("%d",&arr2[i][j]);
       }
    }
    
     printf("AFTER ADDING :\n");
    for(int i=0;i<m;i++){
      for(int j=0;j<n;j++){
           arr3[i][j]=arr1[i][j]+arr2[i][j];
      }
    }
    
    for(int i=0;i<m;i++){
        for(int j=0;j<n;j++){
            printf("%d ",arr3[i][j]);
            
        }
        printf("\n");
    }
    
    return 0;
}
