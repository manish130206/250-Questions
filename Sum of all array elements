#include <stdio.h>

int main() {
    int arr[20], n, i, sum = 0;

    
    printf("Enter the number of elements in the array: ");
    scanf("%d", &n);

    printf("Enter the elements of the array:\n");
    for (i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }

    for (i = 0; i < n; i++) {
        sum += arr[i];
    }

    printf("Sum of all array elements: %d\n", sum);

    return 0;
}
